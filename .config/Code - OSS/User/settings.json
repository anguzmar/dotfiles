{
    "editor.fontFamily": "'Monaco','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 14,
    "editor.wordWrapColumn": 120,
    "editor.lineNumbers": "relative",
    "workbench.colorTheme": "Night Owl",
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "workbench.editor.showTabs": true,
    "workbench.startupEditor": "newUntitledFile",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "d"],
            "commands": ["editor.action.peekDefinition"]
        },
        {
            "before": ["<leader>", "b"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"]
        },
    ],
    "terminal.integrated.fontFamily": "Inconsolata",
    "editor.rulers": [
        120
    ],
    "git.autofetch": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "vim.leader": ",",
    "vim.handleKeys":{
        "<C-d>": true,
        "<C-w>": false,
        "<C-k>": false,
    },
    "vim.textwidth": 120,
    "sync.gist": "014ff4f98b11de2ba52fc67bbc3abcbd",
    "terminal.integrated.shell.linux": "/bin/zsh",
}